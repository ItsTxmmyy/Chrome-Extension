{
  "manifest_version": 3,
  "name": "Therapy Form Extension",
  "version": "1.0",
  "description": "Opens Therapy Forms",
  "permissions": ["activeTab", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "web_accessible_resources": [
    {
      "resources": ["form1.html"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": ["form2.html"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": ["form3.html"],
      "matches": ["<all_urls>"]
    }
  ]
}




